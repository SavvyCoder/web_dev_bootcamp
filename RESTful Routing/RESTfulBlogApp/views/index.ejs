<% include partials/header %>


<h1>INDEX PAGE!!!</h1>

<% blogs.forEach(function(blog){ %>

    <div>
        <h4><%= blog.title %></h4>
        <img src="<%= blog.image %>">
        <p><%- blog.body.substring(0,100) %>...</p>
        <p><%= blog.created %></p>
        <a href= "/blogs/<%= blog._id %>">Read More</a>
    </div>

<% }) %>

<% include partials/footer %>